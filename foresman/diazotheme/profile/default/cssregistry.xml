<?xml version="1.0"?>
<object name="portal_css">

 <!-- Set conditions on stylesheets we don't want to pull in -->
 <stylesheet
     expression="not:request/HTTP_X_THEME_ENABLED | nothing"
     id="public.css"
     />

 <!-- Add new stylesheets -->
 <!-- Note: applyPrefix is not available in Plone < 4.0b3 -->

 <stylesheet title=""
    enabled="on"
    expression="request/HTTP_X_THEME_ENABLED | nothing"
    id="++theme++foresman.diazotheme/css/styles.css"
    authenticated="False"
    cacheable="True"
    compression="safe"
    conditionalcomment=""
    cookable="True"
    media=""
    rel="stylesheet"
    rendering="link"
    applyPrefix="True"
    />

</object>